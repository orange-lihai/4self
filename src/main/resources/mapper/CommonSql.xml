<?xml version="1.0"  encoding="UTF-8"  ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="CommonSql">
	<select id="selectBySql" parameterType="String" resultType="Map">
		${value}
	</select>
	<insert id="insertBySql" parameterType="Map" useGeneratedKeys="true"
		keyProperty="id">
		${sql}
	</insert>
	<delete id="deleteBySql" parameterType="String">
		${value}
	</delete>
	<update id="updateBySql" parameterType="String">
		${value}
	</update>
</mapper>
